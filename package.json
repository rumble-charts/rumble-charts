{
  "name": "rumble-charts",
  "version": "1.0.0",
  "description": "React charts components based on d3.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "test": "npm run build && ./node_modules/.bin/jest --coverage",
    "start": "./node_modules/.bin/babel --watch src --out-dir lib & ./node_modules/.bin/jest --watch --noStackTrace",
    "check": "./node_modules/.bin/eslint src/*; exit 0",
    "gh-pages": "./node_modules/.bin/gh-pages -d styleguide",
    "styleguide-server": "styleguidist server",
    "styleguide-build": "NODE_ENV=production styleguidist build",
    "publish-patch": "npm test && npm version patch && npm publish",
    "publish-minor": "npm test && npm version minor && npm publish",
    "publish-major": "npm test && npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RumbleInc/rumble-js-charts.git"
  },
  "keywords": [
    "react",
    "d3",
    "chart",
    "react-component",
    "bar chart",
    "pie chart",
    "line chart",
    "tags cloud",
    "scatter plot",
    "animated charts",
    "graph",
    "visualization",
    "svg"
  ],
  "author": "Alexey Volkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RumbleInc/rumble-js-charts/issues"
  },
  "homepage": "https://github.com/RumbleInc/rumble-js-charts#readme",
  "dependencies": {
    "d3": "3.5.16",
    "d3-cloud": "1.2.1",
    "lodash": "3.10.1"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-jest": "10.0.1",
    "babel-plugin-transform-react-jsx": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react-hmre": "^1.1.1",
    "canvas": "^1.3.12",
    "enzyme": "2.2.0",
    "eslint": "^2.6.0",
    "eslint-plugin-react": "^4.2.3",
    "gh-pages": "^0.11.0",
    "jest": "0.1.40",
    "jest-cli": "0.10.0",
    "jsdom": "^8.3.1",
    "react": "0.14.x",
    "react-addons-test-utils": "0.14.x",
    "react-dom": "0.14.x",
    "react-styleguidist": "sapegin/react-styleguidist#master"
  },
  "peerDependencies": {
    "react": "0.14.x",
    "react-dom": "0.14.x"
  },
  "jest": {
    "automock": false,
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "tests",
    "testPathDirs": [
      "<rootDir>/tests"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys",
      "object.values",
      "es-abstract"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fakeData/",
      "[^_spec]\\.js"
    ],
    "setupTestFrameworkScriptFile": "./tests/init.js"
  }
}
