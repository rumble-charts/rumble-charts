{
  "name": "rumble-charts",
  "version": "4.0.0",
  "description": "Truly declarative React charts components",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "browser": "umd/rumble-charts.js",
  "scripts": {
    "all": "npm run lint && npm test && npm run build && npm run docs",
    "build": "npm run build:clear && npm run build:dist && npm run build:types",
    "build:clear": "rimraf ./cjs ./es ./types ./umd",
    "build:dist": "rollup --config rollup.config.js",
    "build:types": "tsc --jsx react --declaration --emitDeclarationOnly --outDir types --esModuleInterop src/index.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "deploy:docs": "gh-pages -d storybook -r git@github.com:rumble-charts/rumble-charts.github.io.git -b master",
    "docs": "npm run docs:clear && npm run docs:storybook && npm run docs:playroom && npm run docs:types",
    "docs:clear": "rimraf ./storybook",
    "docs:playroom": "playroom build",
    "docs:storybook": "build-storybook -o storybook",
    "docs:types": "typedoc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "concurrently \"npm run start:storybook\" \"npm run start:playroom\"",
    "start:playroom": "playroom start",
    "start:storybook": "start-storybook -p 6006",
    "prepare": "husky install",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rumble-charts/rumble-charts.git"
  },
  "files": [
    "cjs/*",
    "es/*",
    "types/*",
    "umd/*"
  ],
  "keywords": [
    "react",
    "d3",
    "chart",
    "react-component",
    "bar chart",
    "pie chart",
    "line chart",
    "tags cloud",
    "scatter plot",
    "animated charts",
    "graph",
    "visualization",
    "svg"
  ],
  "author": "Alexey Volkov <a@insvit.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rumble-charts/rumble-charts/issues"
  },
  "homepage": "https://rumble-charts.github.io/",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/components": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@storybook/theming": "^6.3.7",
    "@types/d3-ease": "^3.0.0",
    "@types/d3-interpolate": "^3.0.1",
    "@types/d3-scale": "^4.0.1",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/d3-shape": "^3.0.2",
    "@types/d3-timer": "^3.0.0",
    "@types/eslint": "^7.28.0",
    "@types/estree": "^0.0.50",
    "@types/jest": "^27.0.1",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "concurrently": "^6.2.1",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-react": "^7.24.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lz-string": "^1.4.4",
    "playroom": "^0.27.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-element-to-jsx-string": "^14.3.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-rename": "^1.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "storybook-addon-playroom": "^2.1.2",
    "tslib": "^2.3.1",
    "typedoc": "^0.21.6",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "d3-cloud": "^1.2.5",
    "d3-color": "^3.0.1",
    "d3-ease": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.0",
    "d3-scale-chromatic": "^3.0.0",
    "d3-shape": "^3.0.1",
    "d3-timer": "^3.0.1"
  }
}
